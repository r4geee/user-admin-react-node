{
  "name": "user-admin-react-node",
  "version": "1.0.0",
  "description": "Example express-react app",
  "main": "index.js",
  "scripts": {
    "devclient": "cd client && npm run start",
    "devserver": "./node_modules/nodemon/bin/nodemon.js src/bin/example-express-react",
    "dev": "concurrently 'npm run devserver' 'npm run devclient'",
    "build": "cd client && npm run build",
    "start": "node src/bin/example-express-react",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "handlebars": "^4.0.12",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "nodemailer": "^4.6.8",
    "password-generator": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.0.1",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*"
    ]
  }
}
