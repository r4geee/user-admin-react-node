  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "handlebars": "^4.0.12",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "nodemailer": "^4.6.8",
    "password-generator": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.4"
  },
  "nodemonConfig": {
    "ignore": [
      "data/*"
    ]
  }
